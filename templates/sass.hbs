/**
 * Blue: {{ blue.hex }}, rgb({{join blue.rgb}}), hsl({{join blue.hsl}})
 * Steel: {{ steel.hex }}, rgb({{join steel.rgb}}), hsl({{join steel.hsl}})
 * Green: {{ green.hex }}, rgb({{join green.rgb}}), hsl({{join green.hsl}})
 * Orange: {{ orange.hex }}, rgb({{join orange.rgb}}), hsl({{join orange.hsl}})
 * Purple: {{ purple.hex }}, rgb({{join purple.rgb}}), hsl({{join purple.hsl}})
 * Cinnamon: {{ cinnamon.hex }}, rgb({{join cinnamon.rgb}}), hsl({{join cinnamon.hsl}})
 * Rose: {{ rose.hex }}, rgb({{join rose.rgb}}), hsl({{join rose.hsl}})
 * Granite: {{ granite.hex }}, rgb({{join granite.rgb}}), hsl({{join granite.hsl}})
 * Basalt: {{ basalt.hex }}, rgb({{join basalt.rgb}}), hsl({{join basalt.hsl}})
 * Bone: {{ bone.hex }}, rgb({{join bone.rgb}}), hsl({{join bone.hsl}})
 * Silver: {{ silver.hex }}, rgb({{join silver.rgb}}), hsl({{join silver.hsl}})
 * Magenta: {{ magenta.hex }}, rgb({{join magenta.rgb}}), hsl({{join magenta.hsl}})
 * Yellow: {{ yellow.hex }}, rgb({{join yellow.rgb}}), hsl({{join yellow.hsl}})
 * Cyan: {{ cyan.hex }}, rgb({{join cyan.rgb}}), hsl({{join cyan.hsl}})
 */

$color--blue: {{ blue.hex }};
$color--steel: {{ steel.hex }};
$color--green: {{ green.hex }};
$color--orange: {{ orange.hex }};
$color--purple: {{ purple.hex }};
$color--cinnamon: {{ cinnamon.hex }};
$color--rose: {{ rose.hex }};
$color--granite: {{ granite.hex }};
$color--basalt: {{ basalt.hex }};
$color--bone: {{ bone.hex }};
$color--silver: {{ silver.hex }};
$color--magenta: {{ magenta.hex }};
$color--yellow: {{ yellow.hex }};
$color--cyan: {{ cyan.hex }};
